{
  "name": "vanilla-parcel-boilerplate",
  "version": "1.0.0",
  "description": "Simple workflow starter for building vanilla JS apps",
  "main": "index.js",
  "scripts": {
    "copy-assets": "mkdir -p development && cp -r public/assets development/",
    "dev": "npm run clean && npm run copy-assets && parcel public/index.html --out-dir development -p 3000 --public-url /",
    "build": "rimraf ./dist && parcel build public/*.html --out-dir dist --public-url ./ && cp -r public/assets dist/",
    "clean": "rimraf ./development && rimraf ./.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradtraversy/vanilla-parcel-boilerplate.git"
  },
  "keywords": [
    "parcel"
  ],
  "author": "Brad Traversy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bradtraversy/vanilla-parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/bradtraversy/vanilla-parcel-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/runtime-corejs2": "^7.9.2",
    "parcel-bundler": "^1.12.4",
    "rimraf": "^3.0.2",
    "sass": "^1.26.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^5.3.8",
    "react": "^19.1.1"
  }
}
